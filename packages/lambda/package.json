{
  "name": "@maracanaunews/lambda",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --check .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist",
    "dev": "tsc --project tsconfig.json --watch"
  },
  "devDependencies": {
    "@maracanaunews/eslint-config": "workspace:^",
    "@maracanaunews/prettier-config": "workspace:^",
    "@maracanaunews/tsconfig": "workspace:^",
    "@maracanaunews/vitest-config": "workspace:^",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
